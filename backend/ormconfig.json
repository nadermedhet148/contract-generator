{
   "database": ":memory:",
   "type": "sqlite",
   "name": "default",
   "synchronize": true,
   "logging": true,
   "entities": [
      "src/Infrastructure/Database/Entities/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}